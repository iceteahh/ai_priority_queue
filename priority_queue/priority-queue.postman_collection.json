      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"ad\": {\n    \"adId\": \"ad_102\",\n    \"gameFamily\": \"Shooter\",\n    \"priority\": 1,\n    \"maxWaitTime\": 60\n  },\n  \"enqueueAt\": \"{{$isoTimestamp}}\"\n}\n"
        },
        "url": {
          "raw": "{{baseUrl}}/enqueue",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "enqueue"
          ]
        }
      }
