{
  "info": {
    "name": "Priority Queue HTTP API",
    "_postman_id": "0fe634aa-b75d-4378-ba2b-ceafd9db9026",
    "description": "Postman collection for the Priority Queue service.\nIncludes health, enqueue/dequeue, peek, distribution, waiting filters,\nreprioritization, and settings endpoints.",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:8080"
    }
  ],
  "item": [
    {
      "name": "Health",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{baseUrl}}/healthz",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "healthz"
          ]
        }
      }
    },
    {
      "name": "Enqueue (server time)",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"ad\": {\n    \"adId\": \"ad_101\",\n    \"title\": \"Dragon\",\n    \"gameFamily\": \"RPG\",\n    \"targetAudience\": [\n      \"18-34\"\n    ],\n    \"priority\": 2,\n    \"createdAt\": \"2025-01-01T00:00:00Z\",\n    \"maxWaitTime\": 120\n  }\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/enqueue",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "enqueue"
          ]
        }
      }
    },
